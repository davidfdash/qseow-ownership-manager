# QSEoW Ownership Manager Configuration

# PostgreSQL Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=qseow_ownership
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-secure-password

# Encryption key for storing certificate paths securely
# Generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
ENCRYPTION_KEY=your-fernet-encryption-key

# Optional: Default QSEoW Server Configuration (for single-server mode)
# These are used as fallback if no servers are configured in the database
# QSEOW_SERVER_URL=https://qlikserver.domain.com:4242
# QSEOW_CERT_PATH=/path/to/client.pem
# QSEOW_KEY_PATH=/path/to/client_key.pem
# QSEOW_ROOT_CERT_PATH=/path/to/root.pem
# QSEOW_USER_DIRECTORY=INTERNAL
# QSEOW_USER_ID=sa_api
